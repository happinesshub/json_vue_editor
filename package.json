{
  "name": "json-editor-vue",
  "version": "0.6.3",
  "private": false,
  "description": "JSON editor for Vue 2.6 / 2.7 / 3.",
  "author": "cloydlau",
  "license": "ISC",
  "repository": "https://github.com/cloydlau/json-editor-vue",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "editor",
    "viewer",
    "vue",
    "vue2",
    "vue3",
    "vue-demi"
  ],
  "exports": {
    ".": {
      "require": "./dist/json-editor-vue.umd.js",
      "import": "./dist/json-editor-vue.mjs",
      "browser": "./dist/json-editor-vue.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "unpkg": "./dist/json-editor-vue.umd.js",
  "jsdelivr": "./dist/json-editor-vue.umd.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "serve": "vite preview",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "cl switchVue 3 && vitest run && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vanilla-jsoneditor": "0",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@cloydlau/scripts": "latest",
    "@types/lodash-es": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitest/coverage-c8": "latest",
    "@vitest/ui": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "case-police": "latest",
    "eslint": "latest",
    "happy-dom": "latest",
    "husky": "latest",
    "jsdom": "latest",
    "json-editor-vue": "latest",
    "lint-staged": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest",
    "vue": "latest"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}