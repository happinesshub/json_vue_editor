{
  "name": "json-editor-vue",
  "PascalCasedName": "JsonEditorVue",
  "version": "0.12.0",
  "private": false,
  "description": "Vue 2.6/2.7/3 & Nuxt 2/3 isomorphic JSON editor, viewer, formatter and validator.",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": "https://github.com/cloydlau/json-editor-vue",
  "keywords": [
    "ajv",
    "bigint",
    "edit",
    "format",
    "formatter",
    "json",
    "jsoneditor",
    "nuxt",
    "nuxtjs",
    "schema",
    "ssr",
    "svelte-jsoneditor",
    "validate",
    "validation",
    "validator",
    "view",
    "vue",
    "vue-demi",
    "vue-json-pretty",
    "vue-json-viewer"
  ],
  "exports": {
    ".": {
      "types": "./dist/json-editor-vue.d.ts",
      "import": "./dist/json-editor-vue.mjs",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "unpkg": "./dist/json-editor-vue.umd.js",
  "jsdelivr": "./dist/json-editor-vue.umd.js",
  "types": "./dist/json-editor-vue.d.ts",
  "files": [
    "dist",
    "docs",
    "nuxt.ts"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "test-unit": "vitest run",
    "coverage": "vitest run --coverage",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "release": "esno ./scripts/release.mts",
    "lint": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\"",
    "lint:fix": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\" --fix",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vanilla-jsoneditor": "^0.21.5",
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.6",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^1.2.2",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/test-utils": "^2.4.4",
    "case-police": "^0.6.1",
    "cross-spawn": "^7.0.3",
    "del": "^7.1.0",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.56.0-0",
    "esno": "^4.0.0",
    "happy-dom": "^13.3.8",
    "json-editor-vue": "workspace:*",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.2",
    "lodash-es": "^4.17.21",
    "lossless-json": "^4.0.1",
    "magicast": "^0.3.3",
    "only-allow": "^1.2.1",
    "open": "^10.0.3",
    "prompts": "^2.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "semver": "^7.6.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitepress": "1.0.0-rc.41",
    "vitest": "^1.2.2",
    "vue": "^3.4.15",
    "vue-global-config": "^0.6.0",
    "zhlint": "^0.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": "case-police --fix",
    "docs/README.zh-CN.md": "zhlint --fix",
    "*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}": "eslint --cache --fix"
  }
}
