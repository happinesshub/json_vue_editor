{
  "name": "json-editor-vue",
  "version": "0.3.6",
  "author": "cloydlau",
  "license": "ISC",
  "description": "JSON editor for Vue 2 and 3 powered by svelte-jsoneditor",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "editor",
    "viewer",
    "vue",
    "vue2",
    "vue3"
  ],
  "private": false,
  "main": "dist/json-editor-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/json-editor-vue.es.js",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/json-editor-vue"
  },
  "scripts": {
    "dev": "vue-demi-switch 3 && vite",
    "dev2": "vue-demi-switch 2 && vite",
    "build": "vite build",
    "serve": "vite preview",
    "c": "rd /s /q .\\node_modules",
    "ri": "npm run c && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward"
  },
  "dependencies": {
    "clipboard": "latest",
    "jsonrepair": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/composition-api": "latest",
    "chalk": "4.1.2",
    "enquirer": "latest",
    "execa": "4.1.0",
    "minimist": "latest",
    "semver": "latest",
    "svelte-jsoneditor": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue": "latest"
  }
}
