{
  "name": "json-editor-vue",
  "version": "0.4.13",
  "private": false,
  "description": "JSON editor for Vue 2 & 3",
  "author": "cloydlau",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/json-editor-vue"
  },
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "editor",
    "viewer",
    "vue",
    "vue2",
    "vue3",
    "vue-demi"
  ],
  "exports": {
    ".": {
      "require": "./dist/json-editor-vue.umd.js",
      "import": "./dist/json-editor-vue.mjs"
    }
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "scripts": {
    "dev": "node scripts/switch-vue.js 3 && vite --config ./vite-vue3.config.ts --open",
    "dev2": "node scripts/switch-vue.js 2 && vite --config ./vite-vue2.config.ts --open",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/u.js",
    "r": "node scripts/r.js",
    "rs": "node scripts/r.js --straightforward",
    "test": "vitest --config ./vitest.config.ts",
    "test2": "vitest --config ./vitest.vue2.config.ts",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "latest",
    "vanilla-jsoneditor": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/test-utils": "1",
    "chalk": "4.1.2",
    "element-ui": "^2.15.9",
    "enquirer": "latest",
    "eslint": "latest",
    "execa": "4.1.0",
    "husky": "latest",
    "lint-staged": "latest",
    "minimist": "latest",
    "semver": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue": "2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
