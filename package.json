{
  "name": "json-editor-vue",
  "PascalCasedName": "JsonEditorVue",
  "version": "0.10.5",
  "private": false,
  "packageManager": "pnpm@7.26.1",
  "description": "JSON editor & viewer for Vue 2.6/2.7/3 & Nuxt 2/3.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/json-editor-vue",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "json-editor",
    "vue",
    "vue-demi",
    "vue-json-viewer",
    "nuxt",
    "nuxtjs",
    "ssr"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/json-editor-vue.umd.js",
      "import": "./dist/json-editor-vue.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "unpkg": "./dist/json-editor-vue.umd.js",
  "jsdelivr": "./dist/json-editor-vue.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "nuxt.ts"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "test-unit": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "npx lint-staged && pnpm test-unit && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vanilla-jsoneditor": ">=0",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.13.11",
    "vue-global-config": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.8",
    "case-police": "^0.5.10",
    "happy-dom": "^8.1.5",
    "husky": "^8.0.3",
    "json-editor-vue": "^0.10.5",
    "lint-staged": "^13.1.0",
    "lossless-json": "^2.0.5",
    "rome": "^11.0.0",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.2",
    "vanilla-jsoneditor": "^0.11.8",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vitepress": "1.0.0-alpha.32",
    "vitest": "^0.25.8",
    "vue": "^3.2.45",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": "rome check",
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}