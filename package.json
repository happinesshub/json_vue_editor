{
  "name": "json-editor-vue",
  "PascalCasedName": "JsonEditorVue",
  "version": "0.10.9",
  "private": false,
  "description": "JSON editor & viewer for Vue 2.6/2.7/3 & Nuxt 2/3.",
  "author": "Cloyd Lau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/json-editor-vue",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "json-editor",
    "vue",
    "vue-demi",
    "vue-json-viewer",
    "nuxt",
    "nuxtjs",
    "ssr"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/json-editor-vue.mjs",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "unpkg": "./dist/json-editor-vue.umd.js",
  "jsdelivr": "./dist/json-editor-vue.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "nuxt.ts"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks",
    "dev": "esno ./scripts/dev.mts",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "test-unit": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "esno ./scripts/release.mts"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vanilla-jsoneditor": ">=0",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "simple-git-hooks": "latest",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/cross-spawn": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@types/prompts": "latest",
    "@types/semver": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitest/ui": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/composition-api": "latest",
    "@vue/test-utils": "latest",
    "case-police": "latest",
    "cross-spawn": "latest",
    "eslint": "latest",
    "esno": "latest",
    "happy-dom": "latest",
    "json-editor-vue": "workspace:*",
    "kolorist": "latest",
    "lint-staged": "latest",
    "lodash-es": "latest",
    "lossless-json": "latest",
    "magicast": "latest",
    "prompts": "latest",
    "semver": "latest",
    "typescript": "latest",
    "vanilla-jsoneditor": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitepress": "latest",
    "vitest": "latest",
    "vue": "latest",
    "vue-global-config": "latest",
    "zhlint": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": "case-police --fix",
    "docs/README.zh-CN.md": "zhlint --fix",
    "*": "eslint --cache --ignore-pattern 'demo/' --fix"
  }
}
