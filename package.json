{
  "name": "json-editor-vue",
  "version": "0.2.1",
  "author": "cloydlau",
  "license": "ISC",
  "description": "Optionated json editor & json viewer powered by svelte-jsoneditor & vue-json-viewer",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "editor",
    "viewer",
    "vue"
  ],
  "private": false,
  "main": "dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/json-editor-vue.es.js",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/json-editor-vue"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i"
  },
  "dependencies": {
    "jsonrepair": "latest",
    "kayran": "latest",
    "lodash-es": "latest"
  },
  "peerDependencies": {
    "svelte-jsoneditor": "latest",
    "vue": "2",
    "vue-json-viewer": "latest"
  },
  "devDependencies": {
    "element-ui": "latest",
    "sass": "latest",
    "svelte-jsoneditor": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-vue2": "latest",
    "vite-plugin-windicss": "latest",
    "vue": "^2.6.14",
    "vue-json-viewer": "latest",
    "vue-template-compiler": "latest"
  }
}
