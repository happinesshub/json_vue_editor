{
  "name": "json-editor-vue",
  "version": "0.4.10",
  "author": "cloydlau",
  "license": "ISC",
  "description": "JSON editor for Vue 2 & 3",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "json",
    "editor",
    "viewer",
    "vue",
    "vue2",
    "vue3",
    "vue-demi"
  ],
  "private": false,
  "main": "dist/json-editor-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/json-editor-vue.es.js",
      "require": "./dist/json-editor-vue.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/json-editor-vue"
  },
  "scripts": {
    "dev": "node scripts/switch-vue.js 3 && vite --config ./vite-vue3.config.ts --open",
    "dev2": "node scripts/switch-vue.js 2 && vite --config ./vite-vue2.config.ts --open",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward",
    "test": "vitest --config ./vitest.config.ts",
    "test2": "vitest --config ./vitest.vue2.config.ts",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "lodash-es": "latest",
    "svelte-jsoneditor": "latest",
    "vue-demi": "latest",
    "vue-global-config": "latest"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/test-utils": "1",
    "chalk": "4.1.2",
    "element-ui": "^2.15.9",
    "enquirer": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "execa": "4.1.0",
    "husky": "latest",
    "lint-staged": "latest",
    "minimist": "latest",
    "prettier": "latest",
    "semver": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue": "2"
  }
}
