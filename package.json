{
  "name": "json-editor-vue",
  "version": "0.3.0",
  "author": "cloydlau",
  "license": "ISC",
  "description": "JSON editor & viewer for Vue 2 and 3 powered by svelte-jsoneditor & vue-json-viewer.",
  "keywords": [
    "jsoneditor",
    "svelte-jsoneditor",
    "vue-json-viewer",
    "vue3-json-viewer",
    "json",
    "editor",
    "viewer",
    "vue",
    "vue2",
    "vue3"
  ],
  "private": false,
  "main": "dist/json-editor-vue.umd.js",
  "module": "dist/json-editor-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/json-editor-vue.es.js",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/json-editor-vue"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i"
  },
  "dependencies": {
    "clipboard": "latest",
    "jsonrepair": "latest",
    "kayran": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest",
    "vue-json-viewer": "latest",
    "vue3-json-viewer": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "*",
    "svelte-jsoneditor": "*",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "svelte-jsoneditor": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "element-plus": "1.1.0-beta.12",
    "sass": "latest",
    "svelte-jsoneditor": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-windicss": "latest",
    "vue": "^3.2.13"
  }
}
