    "svelte-jsoneditor": "^0.0.19",
    "vue-json-viewer": "^2.2.18"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "element-ui": "^2.15.1",
    "sass": "^1.32.12",
    "typescript": "^4.2.4",
    "vite": "^2.2.4",
