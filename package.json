{
  "name": "json-editor-vue",
  "PascalCasedName": "JsonEditorVue",
  "version": "0.11.2",
  "private": false,
  "description": "Vue 2.6/2.7/3 & Nuxt 2/3 isomorphic JSON editor, viewer, formatter and validator.",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": "https://github.com/cloydlau/json-editor-vue",
  "keywords": [
    "ajv",
    "bigint",
    "edit",
    "format",
    "formatter",
    "json",
    "jsoneditor",
    "nuxt",
    "nuxtjs",
    "schema",
    "ssr",
    "svelte-jsoneditor",
    "validate",
    "validation",
    "validator",
    "view",
    "vue",
    "vue-demi",
    "vue-json-pretty",
    "vue-json-viewer"
  ],
  "exports": {
    ".": {
      "types": "./dist/json-editor-vue.d.ts",
      "import": "./dist/json-editor-vue.mjs",
      "require": "./dist/json-editor-vue.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/json-editor-vue.umd.js",
  "module": "./dist/json-editor-vue.mjs",
  "unpkg": "./dist/json-editor-vue.umd.js",
  "jsdelivr": "./dist/json-editor-vue.umd.js",
  "types": "./dist/json-editor-vue.d.ts",
  "files": [
    "dist",
    "docs",
    "nuxt.ts"
  ],
  "scripts": {
    "dev": "esno ./scripts/dev.mts",
    "test-unit": "vitest run",
    "coverage": "vitest run --coverage",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "release": "esno ./scripts/release.mts",
    "lint": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\"",
    "lint:fix": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\" --fix",
    "pnpm:devPreinstall": "node ./scripts/devPreinstall.mjs",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vanilla-jsoneditor": "^0.21.2",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/cross-spawn": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@types/prompts": "latest",
    "@types/semver": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitest/ui": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "case-police": "latest",
    "cross-spawn": "latest",
    "del": "latest",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-plugin-format": "latest",
    "eslint-ts-patch": "8.56.0-0",
    "esno": "latest",
    "happy-dom": "latest",
    "json-editor-vue": "workspace:*",
    "kolorist": "latest",
    "lint-staged": "latest",
    "lodash-es": "latest",
    "lossless-json": "latest",
    "magicast": "latest",
    "only-allow": "latest",
    "open": "latest",
    "prompts": "latest",
    "rollup-plugin-visualizer": "latest",
    "semver": "latest",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "unplugin-vue-components": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitepress": "1.0.0-rc.39",
    "vitest": "latest",
    "vue": "latest",
    "vue-global-config": "latest",
    "zhlint": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": "case-police --fix",
    "docs/README.zh-CN.md": "zhlint --fix",
    "*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}": "eslint --cache --fix"
  }
}
